{
	"manifest_version":3,
	"name":"dev",
	"version":"0.0.0",
	"permissions": [
		"tabs",
		"input",
		"storage"

	],
	"background":{
		"service_worker":"./service.js"
	},
	"action": {
		"default_title": "Click to view a popup",
		"default_popup": "popup.html"
	},

	"commands": {
		"prefix":{
			"suggested_key":"Ctrl+Shift+Q",
			"description":"prefix"
		}
	}
}
